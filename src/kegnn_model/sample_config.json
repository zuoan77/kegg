{
  "data": {
    "data_dir": "../../output/graph_builder_output",
    "nodes_file": "abstract_nodes_summary.csv",
    "connections_file": "abstract_node_connections.csv",
    "edge_weights_file": "edge_weights.json",
    "test_ratio": 0.2,
    "val_ratio": 0.1,
    "negative_sampling_ratio": 1.0,
    "max_ko_numbers": 1000,
    "max_compounds": 500,
    "num_samples": [
      25,
      10
    ],
    "batch_size": 256,
    "num_workers": 4
  },
  "model": {
    "model_type": "graphsage",
    "input_dim": 128,
    "hidden_dims": [
      128,
      64
    ],
    "output_dim": 32,
    "dropout": 0.5,
    "aggregator_type": "mean",
    "num_heads": 8,
    "link_pred_hidden_dims": [
      64,
      32
    ],
    "device": "cuda"
  },
  "training": {
    "epochs": 200,
    "learning_rate": 0.01,
    "weight_decay": 0.0005,
    "scheduler_type": "step",
    "step_size": 50,
    "gamma": 0.5,
    "patience": 10,
    "early_stopping": true,
    "early_stopping_patience": 20,
    "min_delta": 0.0001,
    "save_model": true,
    "save_dir": "../../kegnn_model/checkpoints",
    "save_best_only": true,
    "log_interval": 10,
    "eval_interval": 5
  },
  "evaluation": {
    "metrics": [
      "accuracy",
      "auc",
      "precision",
      "recall",
      "f1"
    ],
    "plot_training_curves": true,
    "plot_confusion_matrix": true,
    "plot_roc_curve": true,
    "results_dir": "../../kegnn_model/results",
    "save_predictions": true
  },
  "experiment_name": "sample_experiment",
  "seed": 42
}